{
  "name": "uaop-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:web": "cd apps/web && pnpm dev",
    "dev:api": "cd apps/api && uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "dev": "concurrently \"pnpm dev:api\" \"pnpm dev:web\"",
    "build:web": "cd apps/web && pnpm build",
    "install:all": "pnpm install && cd apps/api && pip install -r requirements.txt"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
